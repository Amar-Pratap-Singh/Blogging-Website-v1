<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">

  <div class="form-group">
    <label for="textTitle">Title</label>
    <input id="textTitle" class="form-control" type="text" name="textTitle">
  </div>

  <div class="form-group">
    <label for="textBody">Post</label>
    <textarea id="textBody" class="form-control" name="textBody" rows="8" cols="80"></textarea>
  </div>

  <button type="submit" name="button" class="btn btn-primary">Publish</button>


</form>

<%- include("partials/footer") -%>
